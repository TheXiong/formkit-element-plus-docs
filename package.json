{
  "name": "formkit-element-plus-docs",
  "version": "1.0.0",
  "type": "module",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "docs:dev": "pnpm run register:components && vitepress dev docs",
    "docs:build": "pnpm run register:components && vitepress build docs --base /formkit-element-plus-docs/",
    "docs:serve": "vitepress serve docs",
    "register:components": "vitepress-rc",
    "deploy": "zsh scripts/gh-pages.sh"
  },
  "devDependencies": {
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "less": "^4.2.0",
    "prettier": "^3.2.5",
    "vitepress": "^1.0.1",
    "vitepress-theme-demoblock": "^3.0.5"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search"
      ]
    }
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "@formkit/i18n": "^1.6.7",
    "@formkit/vue": "^1.6.7",
    "element-plus": "^2.8.6",
    "formkit-element-plus": "1.0.22"
  }
}
